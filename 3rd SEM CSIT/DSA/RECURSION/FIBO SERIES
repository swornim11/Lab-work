#include<iostream>
using namespace std;

int fibo(int n){
	cout<<"\nfibo("<<n<<")called"<<endl;
	if(n==0){
		cout<<"Returning 0"<<endl;
	    return 0;
	}

    if(n==1){
      cout<<"Returning 1"<<endl;
	  return 1;
	}
    
	  int res= fibo(n-1)+fibo(n-2);
	  cout<<"Returning "<< res<<" from fibo ("<<n<<")"<<endl;
	  return res;
}

int main(){
	int n;
	cout<<"-----Swornim Maharjan----"<<endl;
	cout<<"Enter the number: ";
	cin>>n;
	
	cout<<"\n-------------Recursive Steps-----------"<<endl;
	int series[n];
	for(int i=0;i<n;i++){
	   series[i]=fibo(i);
	}
	cout<<"\nFibonacci series of "<<n<<" terms : "<<endl;
	for(int i=0;i<n;i++){
		cout<<series[i]<<" ";
	}
	
return 0;
}
